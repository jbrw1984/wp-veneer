{
  "name": "wp-veneer",
  "repository": {
    "type": "git",
    "urL": "https://github.com/UsabilityDynamics/wp-veneer"
  },
  "version": "0.1.1",
  "dependencies": {
    "usabilitydynamics/lib-api": "*",
    "usabilitydynamics/lib-ui": "*",
    "usabilitydynamics/lib-loader": "*",
    "usabilitydynamics/lib-cdn": "*",
    "usabilitydynamics/lib-utlity": "*",
    "usabilitydynamics/lib-settings": "*",
    "usabilitydynamics/lib-models": "*",
    "usabilitydynamics/lib-saas": "*"
  },
  "config": {
    "namespace": "Veneer"
  },
  "build": {
    "scripts": {
      "out": "public/scripts",
      "settings": {
        "minify": true
      }
    },
    "styles": {
      "out": "public/styles",
      "settings": {
        "minify": true
      }
    },
    "libs": {
      "out": "core/vendor",
      "settings": {
        "autoload": true
      }
    }
  },
  "license": "MIT"
}
