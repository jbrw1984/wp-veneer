{
  "name": "usabilitydynamics/wp-veneer",
  "type": "library",
  "description": "Veneer network",
  "repository": {
    "type": "git",
    "urL": "https://github.com/UsabilityDynamics/wp-veneer"
  },
  "homepage": "https://github.com/UsabilityDynamics/wp-veneer",
  "keywords": [
    "wordpress",
    "multisite",
    "varnish"
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Andy Potanin",
      "homepage": "http://usabilitydynamics.com"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/UsabilityDynamics/lib-models"
    },
    {
      "type": "git",
      "url": "https://github.com/UsabilityDynamics/lib-settings"
    },
    {
      "type": "git",
      "url": "https://github.com/UsabilityDynamics/lib-utility"
    },
    {
      "type": "git",
      "url": "https://github.com/UsabilityDynamics/lib-saas"
    },
    {
      "type": "git",
      "url": "https://github.com/UsabilityDynamics/lib-cdn"
    },
    {
      "type": "git",
      "url": "https://github.com/UsabilityDynamics/lib-ui"
    },
    {
      "type": "git",
      "url": "https://github.com/UsabilityDynamics/lib-asset"
    },
    {
      "type": "git",
      "url": "https://github.com/UsabilityDynamics/lib-api"
    }
  ],
  "require": {
    "php": ">=5.3",
    "UsabilityDynamics/lib-api": "*",
    "UsabilityDynamics/lib-models": "*",
    "UsabilityDynamics/lib-settings": "*",
    "UsabilityDynamics/lib-saas": "*",
    "UsabilityDynamics/lib-ui": "*",
    "UsabilityDynamics/lib-utility": "*",
    "UsabilityDynamics/lib-cdn": "*",
    "UsabilityDynamics/lib-asset": "*",
    "dflydev/markdown": "1.0.*@dev"
  },
  "autoload": {
    "files": [
      "core/class-api.php",
      "core/class-bootstrap.php",
      "core/class-dashboard.php"
    ]
  },
  "config": {
    "namespace": "Veneer"
  },
  "build": {
    "scripts": {
      "out": "public/scripts",
      "settings": {
        "minify": true
      }
    },
    "styles": {
      "out": "public/styles",
      "settings": {
        "minify": true
      }
    },
    "libs": {
      "out": "core/vendor",
      "settings": {
        "autoload": true
      }
    }
  }
}
